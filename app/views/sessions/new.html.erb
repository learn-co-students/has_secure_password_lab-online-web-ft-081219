<h1>Log In</h1>
<%= form_tag ({controller:'sessions', action:'create', method:'post'}) do %>
    <%= label_tag 'user[name]', 'Username' %><br>
    <%= text_field_tag 'user[name]' %><br>

    <%= label_tag 'user[password]', 'Password' %><br>
    <%= text_field_tag 'user[password]' %><br>

    <%= submit_tag "Log In" %>
<% end %>